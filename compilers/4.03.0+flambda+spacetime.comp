opam-version: "1"
version: "4.03.0"
src: "https://github.com/mshinwell/ocaml/tarball/spacetime-4.03"
build: [
  ["./configure" "-prefix" "%{prefix}%" "-spacetime" "-flambda"]
  ["%{make}%" "world"]
  ["%{make}%" "world.opt"]
  ["%{make}%" "install"]
]
packages: ["base-unix" "base-bigarray" "base-threads" "base-raw_spacetime_lib"]
env: [
  [CAML_LD_LIBRARY_PATH = "%{lib}%/stublibs"]
]
